@article{RN309,
   author = {Ma, J. and Xia, J. and Liang, Z. and Chen, X. and Du, Y. and Yan, C. H.},
   title = {Layered Double Hydroxide Hollowcages with Adjustable Layer Spacing for High Performance Hybrid Supercapacitor},
   journal = {Small},
   volume = {17},
   number = {49},
   pages = {2104423},
   note = {Ma, Jiamin
Xia, Jiale
Liang, Zhong
Chen, Xiaoyun
Du, Yaping
Yan, Chun-Hua
eng
21971117/National Natural Science Foundation of China
