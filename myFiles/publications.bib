	author = {Cottrell, Richard S. and Nash, Kirsty L. and Halpern, Benjamin S. and Remenyi, Tomas A. and Corney, Stuart P. and Fleming, Aysha and Fulton, Elizabeth A. and Hornborg, Sara and Johne, Alexandra and Watson, Reg A. and Blanchard, Julia L.},
	month = feb,
	year = {2019},
	note = {Number: 2
Publisher: Nature Publishing Group},
	keywords = {Agriculture, Agroecology, Macroecology, Marine biology, Sustainability},
	pages = {130--137},
	file = {Accepted Version:C\:\\Users\\baldr\\Zotero\\storage\\JQLTNSVU\\Cottrell et al. - 2019 - Food production shocks across land and sea.pdf:application/pdf;Snapshot:C\:\\Users\\baldr\\Zotero\\storage\\QJDIGD5R\\s41893-018-0210-1.html:text/html},
