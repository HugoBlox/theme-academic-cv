 editor = {Pericles Loucopoulos},
 keywords = {papers, sharing, cscw, datamanagement},
 month = {May 12--15},
 pages = {17--32},
 publisher = {Springer Berlin / Heidelberg},
 series = {Lecture Notes in Computer Science},
 title = {Integrating object and agent worlds},
 volume = {593},
 year = {1992}
}

