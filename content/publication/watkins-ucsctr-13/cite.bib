 institution = {University of California Santa Cruz},
 keywords = {papers, datamodel, scripting, storage, systems, software-defined},
 month = {March 16},
 number = {UCSC-SOE-13-02},
 title = {In-Vivo Storage System Development},
 type = {Technical Report},
 year = {2013}
}

